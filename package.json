{
  "name": "angular-appointment-scheduler",
  "version": "1.0.0",
  "description": "Enterprise-Grade Angular Appointment Scheduler with NestJS Backend",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/your-repo-name.git"
  },
  "keywords": [
    "angular",
    "nestjs",
    "appointment",
    "scheduler",
    "calendar",
    "typescript",
    "material"
  ],
  "scripts": {
    "ng": "ng",
    "start:frontend": "ng serve -o",
    "build:frontend": "ng build",
    "watch:frontend": "ng build --watch --configuration development",
    "test:frontend": "ng test",
    "e2e:frontend": "ng e2e",
    "start:backend": "npm run start:dev -w=api",
    "build:backend": "npm run build -w=api",
    "start:dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "test": "npm run test:frontend && npm run test:backend",
    "test:backend": "npm run test -w=api",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "postinstall": "npm install -w=api"
  },
  "workspaces": [
    "api",
    "frontend"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@nestjs/cache-manager": "latest",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/testing": "^10.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/passport-jwt": "^3.0.13",
    "@types/rrule": "latest",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^2.30.0",
    "reflect-metadata": "^0.1.13",
    "rrule": "^2.7.2",
    "rxjs": "~7.8.0",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.17",
    "zone.js": "~0.14.2"
  },
